@model IEnumerable<YueBoAdmin.Models.ReportType>

@{
    ViewBag.Title = "Index";
}

<h2>举报类型</h2>

<p>
    @Html.ActionLink("添加", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.ReportTypeName)
        </th>
        <th>操作</th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.ReportTypeName)
        </td>
        <td>
            @Html.ActionLink("编辑", "Edit", new { id=item.ReportTypeID }) <br />
            @Html.ActionLink("删除", "Delete", new { id=item.ReportTypeID }) 

        </td>
    </tr>
}

</table>
