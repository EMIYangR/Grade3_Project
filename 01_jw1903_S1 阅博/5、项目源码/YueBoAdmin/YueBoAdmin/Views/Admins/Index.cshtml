@model IEnumerable<YueBoAdmin.Models.Admin>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table table-bordered tab-content">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.AdminNumber)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.AdminAccount)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.AdminPwd)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.AdminNumber)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.AdminAccount)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.AdminPwd)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.AdminID }) |
            @Html.ActionLink("Details", "Details", new { id=item.AdminID }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.AdminID })
        </td>
    </tr>
}

</table>
