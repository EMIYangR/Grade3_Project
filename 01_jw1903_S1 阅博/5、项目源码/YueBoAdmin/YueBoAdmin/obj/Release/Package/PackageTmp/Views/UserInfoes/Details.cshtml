@model YueBoAdmin.Models.UserInfo

@{
    ViewBag.Title = "Details";
}

<h2>用户个人信息详情</h2>

<div>
    <h4></h4>
    <hr />
    <dl class="dl-horizontal">
        <dt>
            用户姓名
        </dt>

        <dd>
            @Html.DisplayFor(model => model.UserName)
        </dd>

        <dt>
            用户昵称
        </dt>

        <dd>
            @Html.DisplayFor(model => model.UserNick)
        </dd>

        <dt>
            用户账号
        </dt>

        <dd>
            @Html.DisplayFor(model => model.UserAccount)
        </dd>

        <dt>
            个性签名
        </dt>

        <dd>
            @Html.DisplayFor(model => model.Signature)
        </dd>

        <dt>
            用户密码
        </dt>

        <dd>
            @Html.DisplayFor(model => model.UserPwd)
        </dd>

        <dt>
            用户头像
        </dt>

        <dd>
            @Html.DisplayFor(model => model.UserPic)
        </dd>

        <dt>
            联系电话
        </dt>

        <dd>
            @Html.DisplayFor(model => model.Phone)
        </dd>

        <dt>
            电子邮箱
        </dt>

        <dd>
            @Html.DisplayFor(model => model.Email)
        </dd>

        <dt>
            身份账号
        </dt>

        <dd>
            @Html.DisplayFor(model => model.IDcard)
        </dd>

        <dt>
            性别
        </dt>

        <dd>
            @Html.DisplayFor(model => model.Sex)
        </dd>

        <dt>
            生日
        </dt>

        <dd>
            @Html.DisplayFor(model => model.Birthday)
        </dd>

        <dt>
            注册时间
        </dt>

        <dd>
            @Html.DisplayFor(model => model.RegisterTime)
        </dd>

        <dt>
            信用分
        </dt>

        <dd>
            @Html.DisplayFor(model => model.CreditScore)
        </dd>

        <dt>
            是否禁用
        </dt>

        <dd>
            @Html.DisplayFor(model => model.IsBan)
        </dd>
    </dl>
</div>
<p>
    @Html.ActionLink("返回", "Index")
</p>
